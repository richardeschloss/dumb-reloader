{
  "name": "webpack-build",
  "version": "1.0.0",
  "description": "A sample Webpack build",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "build:webpack:slim": "webpack --config webpack/config.slim.js",
    "build-json-parser": "webpack --config ./webpack.config.json-parser.js",
    "build-all": "npm run build && npm run build-slim && npm run build-json-parser"
  },
  "author": "Damien Arrachequesne",
  "license": "MIT",
  "dependencies": {
    "socket.io-client": "^2.0.2",
    "socket.io-json-parser": "^2.1.0"
  },
  "devDependencies": {
    "strip-loader": "^0.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
